FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value		uniform 300;
    }

    outlet
    {
        type            zeroGradient;
    }

    lowerleftWall
    {
        type            zeroGradient;
    }
    
    obstacle
    {
        type            fixedValue;
        value		uniform 320;
    }
    
    lowerrightWall
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


